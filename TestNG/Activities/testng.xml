<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
	
<suite name="FSTTests" thread-count="4" parallel="classes"> <!-- Test Suite -->
     <!-- Comment out groups to run everything -->
	<groups>
        <run>
            <include name="HeaderTests" />
        </run>
    </groups>
    <parameter name="username" value="admin" />
    <parameter name="password" value="password" />
    
    <test name="FSTActivities"> <!-- Test Classes -->
        <classes>
     <class name="fsttestng.Activity7" />
      <class name="fsttestng.Activity6" />
       <class name="fsttestng.Activity5" />
        <class name="fsttestng.Activity4_DemoOne" />
         <class name="fsttestng.Activity4_DemoTwo" />
          <class name="fsttestng.Activity3" />
           <class name="fsttestng.Activity2" />
           <class name="fsttestng.Activity1" />
             </classes>
    </test>
</suite>